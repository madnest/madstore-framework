{
    "name": "madnest/madstore-framework",
    "description": "madstore framework",
    "keywords": [
        "madnest",
        "madstore-framework"
    ],
    "homepage": "https://github.com/madnest/madstore-framework",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Theimer",
            "email": "theimer@madne.st",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/support": "^7.0|^8.0",
        "laravel/framework": "^7.0|^8.0",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.1",
        "phpunit/phpunit": "^^9.0"
    },
    "autoload": {
        "psr-4": {
            "Madnest\\Madstore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madnest\\Madstore\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Madnest\\Madstore\\MadstoreServiceProvider"
            ],
            "aliases": {
                "Madstore": "Madnest\\Madstore\\MadstoreFacade"
            }
        }
    }
}
